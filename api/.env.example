# API environment variables
ENV=dev
AUTH_STUB=true
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
REGEX_ENABLED=true
CAS_ENABLED=true
MAX_HINT_LEVEL=3
GOOGLE_CLOUD_PROJECT=
FIREBASE_PROJECT_ID=
# When AUTH_STUB=false, the API expects Firebase Admin credentials.
# Set GOOGLE_APPLICATION_CREDENTIALS to a service account JSON path, e.g.:
# export GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# LLM (Gemini via Vertex AI)
ENABLE_LLM=false
LLM_PROVIDER=gemini
LLM_MODEL=gemini-1.5-flash
VERTEX_PROJECT_ID=
VERTEX_LOCATION=asia-southeast1
